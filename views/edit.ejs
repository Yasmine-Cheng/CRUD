            <input type="text" value="<%= cds.Title %>"  name="Title" id="title_<%= cds._id %>">
            <input type="text" value="<%= cds.Artist %>"  name="Artist" id="artist_<%= cds._id %>">
            <input type="text" value="<%= cds.Genre %>"  name="Genre" id="genre_<%= cds._id %>">
          </li>
        <% } %>
      </ul>
